<?xml version="1.0" encoding="utf-8" ?> 

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minWidth="25px"
    android:minHeight="25px"
    android:padding="25px">
    
    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#bfbfbf"
        android:id="@+id/linearLayout1">
        <TextView
            android:text="NewLocation"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:id="@+id/textView1"
            android:layout_gravity="center"
            android:layout_marginLeft="10px"
            android:layout_marginTop="10px" />
        <TextView
            android:text="1.04040"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Latitude1"
            android:textStyle="normal"
            android:layout_marginTop="15px"
            android:layout_marginLeft="10px"
            android:textSize="30px"
            android:layout_marginRight="40px" />
        <TextView
            android:text="1.05050"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Longitude1"
            android:textStyle="normal"
            android:layout_marginTop="15px"
            android:layout_marginLeft="10px"
            android:textSize="30px" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#bfbfbf"
        android:id="@+id/linearLayout2"
        android:layout_marginTop="10px">
        <TextView
            android:text="OldLocation"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:id="@+id/textView2"
            android:layout_gravity="center"
            android:layout_marginLeft="10px"
            android:layout_marginTop="10px" />
        <TextView
            android:text=""
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Latitude2"
            android:textStyle="normal"
            android:layout_marginTop="15px" 
            android:layout_marginLeft="10px"
            android:textSize="30px"
            android:layout_marginRight="40px" />

        <TextView
            android:text=""
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Longitude2"
            android:textStyle="normal"
            android:layout_marginTop="15px" 
            android:layout_marginLeft="10px"
            android:textSize="30px"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#bfbfbf"
        android:id="@+id/linearLayout3"
        android:layout_marginTop="10px">
        <TextView
            android:text="LastCompared"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:id="@+id/textView3"
            android:layout_gravity="center"
            android:layout_marginLeft="10px"
            android:layout_marginTop="10px" />
        <TextView
            android:text=""
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Latitude3"
            android:textStyle="normal"
            android:layout_marginTop="15px" 
            android:layout_marginLeft="10px"
            android:textSize="30px"
            android:layout_marginRight="40px"/>
        <TextView
            android:text=""
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Longitude3"
            android:textStyle="normal"
            android:layout_marginTop="15px" 
            android:layout_marginLeft="10px"
            android:textSize="30px"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#bfbfbf"
        android:id="@+id/linearLayout4"
        android:layout_marginTop="10px">
        <TextView
            android:text="LastCalm"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:id="@+id/textView4"
            android:layout_gravity="center"
            android:layout_marginLeft="10px"
            android:layout_marginTop="10px" />
        <TextView
            android:text=""
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Latitude4"
            android:textStyle="normal"
            android:layout_marginTop="15px" 
            android:layout_marginLeft="10px"
            android:textSize="30px"
            android:layout_marginRight="40px"/>
        <TextView
            android:text=""
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Longitude4"
            android:textStyle="normal"
            android:layout_marginTop="15px" 
            android:layout_marginLeft="10px"
            android:textSize="30px"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#bfbfbf"
        android:id="@+id/linearLayout5"
        android:layout_marginTop="10px">
        <TextView
            android:text="Trip:"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:id="@+id/textView5"
            android:layout_gravity="center"
            android:layout_marginLeft="10px"
            android:layout_marginTop="10px" />
        <TextView
            android:text="Off"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Trip"
            android:textStyle="normal"
            android:layout_marginTop="15px" 
            android:layout_marginLeft="10px"
            android:layout_marginRight="40px"/>
        <TextView
            android:text=""
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/UpdateValue"
            android:textStyle="normal"
            android:layout_marginTop="15px" 
            android:layout_marginLeft="10px"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#bfbfbf"
        android:id="@+id/linearLayout6"
        android:layout_marginTop="10px">
        <TextView
            android:text="Distance"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:id="@+id/textView6"
            android:layout_gravity="center"
            android:layout_marginLeft="10px"
            android:layout_marginTop="10px" />
        <TextView
            android:text=""
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Distance"
            android:textStyle="normal"
            android:layout_marginTop="15px" 
            android:layout_marginLeft="10px"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#bfbfbf"
        android:id="@+id/linearLayout7"
        android:layout_marginTop="10px">
        <TextView
            android:text="Vehicle:"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:id="@+id/textView7"
            android:layout_gravity="center"
            android:layout_marginLeft="10px"
            android:layout_marginTop="10px" />
        <TextView
            android:text=""
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Vehicle"
            android:textStyle="normal"
            android:layout_marginTop="15px" 
            android:layout_marginLeft="10px"
            android:layout_marginRight="40px"/>
        <TextView
            android:text=""
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/Speed"
            android:textStyle="normal"
            android:layout_marginTop="15px" 
            android:layout_marginLeft="10px"/>
    </LinearLayout>
</LinearLayout>